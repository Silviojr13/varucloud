generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" // mant√©m isso aqui
  url      = "file:local.db" // apenas um placeholder para o Prisma CLI
}

model User {
  id        String   @id @default(uuid())
  nome      String
  email     String   @unique
  senha     String
  createdAt DateTime @default(now())

  @@map("User")
}
